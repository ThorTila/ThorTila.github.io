{
  "name": "thortila.github.io",
  "version": "0.1.0",
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router": "^3.0.5",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "npm-run-all -p start:* watch",
    "start:react": "react-scripts start",
    "watch": "npm run build:sass && npm-run-all watch:*",
    "watch:src":
      "node-sass-chokidar src/ -o src/ -m w --watch --recursive --error-bell",
    "watch:pub":
      "node-sass-chokidar public/sass/style.scss -o public/css --watch --recursive --error-bell",
    "build": "npm-run-all build:*",
    "build:react": "react-scripts build",
    "build:sass": "npm-run-all sass:*",
    "sass:src":
      "node-sass-chokidar src/ -o src/ -m w --output-style compressed",
    "sass:pub":
      "node-sass-chokidar public/sass/style.scss -o public/css --output-style compressed",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3"
  }
}
